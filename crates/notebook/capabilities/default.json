{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Default capabilities for notebook windows",
  "windows": ["main", "notebook-*"],
  "permissions": [
    "core:default",
    "core:event:default",
    "core:webview:allow-set-webview-zoom",
    "core:window:allow-set-theme",
    "dialog:default",
    "process:allow-restart",
    "updater:default"
  ]
}
