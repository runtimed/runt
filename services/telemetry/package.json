{
  "name": "runt-telemetry",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "db:create": "wrangler d1 create runt-telemetry",
    "db:migrate": "wrangler d1 execute runt-telemetry --file=./schema.sql",
    "db:migrate:local": "wrangler d1 execute runt-telemetry --local --file=./schema.sql",
    "db:query": "wrangler d1 execute runt-telemetry --command"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240129.0",
    "wrangler": "^3.28.0"
  }
}
