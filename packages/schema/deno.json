{
  "name": "@runt/schema",
  "version": "0.13.0",
  "description": "Anode schema for runtime agents and clients",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/runtimed/anode.git"
  },
  "exports": {
    ".": "./mod.ts"
  },
  "publish": {
    "include": ["mod.ts", "types.ts", "tables.ts", "queries/", "README.md"]
  },
  "compilerOptions": {
    "strict": true,
    "lib": ["ESNext", "DOM"]
  },
  "imports": {
    "fractional-indexing": "npm:fractional-indexing@^3.2.0",
    "@runtimed/schema": "jsr:@runtimed/schema@^0.1.12",
    "jsr:@std/testing": "jsr:@std/testing@^1.0.15"
  },
  "lint": {
    "rules": {
      "exclude": ["no-slow-types"]
    }
  }
}
